{
  "targets": {
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "debug": true
    },
    "release": {
      "outFile": "build/release.wasm",
      "textFile": "build/release.wat",
      "optimize": "speed",
      "converge": true,
      "noAssert": true
    }
  },
  "options": {
    "importMemory": true,
    "noExportMemory": true
  }
}
